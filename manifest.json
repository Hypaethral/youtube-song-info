{
    "manifest_version": 2,
    "name": "Youtube Song Info",
    "version": "0.1",
    "author": "Hypaethral",
    "description": "For streamers who want youtube song info :)",
    "permissions": [
        "downloads",
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}